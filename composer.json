{
    "name": "dragonrun1/encryption-helper",
    "description": "Use to help with the encryption and decryption of GET queries",
    "type": "library",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Michael Cummings",
            "email": "mgcummings@yahoo.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "EncryptionHelper\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Spec\\EncryptionHelper\\": [
                "specs/"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "require": {
        "ext-openssl": "*",
        "php": ">7.0,<7.2"
    },
    "require-dev": {
        "henrikbjorn/phpspec-code-coverage": "~3.0.1",
        "phpspec/nyan-formatters": "~2.0.0",
        "phpspec/phpspec": "~3.0.0"
    }
}
